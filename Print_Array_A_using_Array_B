#include <stdio.h>
#include <stdlib.h>

int main() {
    int n,m,i,j;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    scanf("%d",&m);
    int b[m];
    for(j=0;j<m;j++)
    scanf("%d",&b[j]);
    
        for(j=0;j<m;j++){
           int h=b[j];
           if(h>=0 && h<n)
            printf("%d ",a[h]);
           else
            printf("-1 ");
        }
    return 0;
}
