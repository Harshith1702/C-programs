#include<stdio.h>
int main(){
	int n,m,i,j,sum=0;
	printf("Enter rows and columns ");
	scanf("%d%d",&n,&m);
	int a[n][m];
	printf("\nEnter elements into matrix ");
	for(i=0;i<n;i++){
		for(j=0;j<m;j++){
			scanf("%d",&a[i][j]);
		}
	}   printf("\nSum of each column\n");
		for(j=0;j<m;j++){
			int sum=0;
		for(i=0;i<n;i++){
			sum+=a[i][j];
		}printf("%d\n",sum);
	}
}
